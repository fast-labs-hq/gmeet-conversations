{
  "name": "GMeet Conversation",
  "version": "1.0.0",
  "description": "Get all gmeet conversation",
  "manifest_version": 3,
  "author": "Sagar",
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/*"],
      "js": ["jquery.js", "index.js"],
      "run_at": "document_idle"
    }
  ],
  "background": {
    "service_worker": "script.js"
  },
  "action": {
    "default_title": "Get all gmeet conversation"
  },
  "icons": {
    "128": "images/logo.png"
  }
}
